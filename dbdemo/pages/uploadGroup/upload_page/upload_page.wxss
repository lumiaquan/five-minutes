.container{
  padding: 40rpx 60rpx;
}

.textarea{
  font-size: 28rpx;
  width: 100%;
  height: 400rpx;
}

.submit-btn{
  margin-top: 40rpx;
}

.image-list-group{
  padding-top: 20rpx;
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
}

.image-list-group .image-group{
  margin-right: 5rpx;
  margin-bottom: 5rpx;
  position: relative;
}

.image-group image{
  width: 100%;
  height: 100%;
}

.image-group .close-btn{
  width: 60rpx;
  height: 60rpx;
  position: absolute;
  right: -30rpx;
  top: -30rpx;
  z-index: 1; 
  background-size: 60rpx;
  background-repeat: no-repeat;
  background-position: center;
  background-image: url("data:image/png;base64,");
}

.image-list-group .addImage{
  font-size: 200rpx;
  color: #a9a9a9;
  background: #f2f2f2;
  display: flex;
  justify-content: center;
  align-items: center;
}

.addImage view{
  margin-top: -20rpx;
}