.container {
  padding: 0 40rpx;
}

.container .logo-group {
  display: flex;
  justify-content: center;
  margin-top: 100rpx;
  margin-bottom: 20rpx;
}

.logo-group .logo {
  width: 300rpx;
  height: 300rpx;
}

.container .tips {
  display: flex;
  justify-content: center;
  font-size: 32rpx;
  color: #646464;
}

.container button {
  width: 100%;
  margin-top: 20rpx;
}
