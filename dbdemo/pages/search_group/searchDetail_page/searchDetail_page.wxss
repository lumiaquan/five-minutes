.history-search{
  display: flex;
  align-items: center;
  margin-top: 20rpx;
  border-bottom: 1rpx solid #b7bdb877;
  padding-bottom: 20rpx; 
}
.history-search image{
  width: 40rpx;
  height: 40rpx;
  margin-right: 20rpx; 
  margin-left: 20rpx;
}
.history-search text{
  font-size: 30rpx;

}
.top-title{
  display: flex;
  justify-content: space-between;
}
.history-title{
  margin-top: 40rpx;
  font-size: 26rpx;
  margin-left: 15rpx; 
  font-weight: 700;
}
.clear{
  margin-top: 40rpx;
  font-size: 30rpx;
  margin-right: 15rpx; 
  font-weight: 500;
  color: #0070c1;
}

.searchbar{
  height: 90rpx;
  background-color: #0070c1;
  box-sizing: border-box;
  padding: 15rpx 20rpx;
}
.search-group{
  width: 100%;
  height: 60rpx;
  background-color: white;
  border-radius: 10rpx;
  padding: 0px 20rpx;
  box-sizing: border-box;
}
.search-input{
  font-size: 12px;
}
